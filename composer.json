{
  "name": "phn/ontome",
  "license": "MIT License",
  "type": "project",
  "autoload": {
    "psr-4": {
      "AppBundle\\": "src/AppBundle",
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\AppBundle\\": "tests/AppBundle",
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.2",
    "doctrine/doctrine-migrations-bundle": "^1.2",
    "doctrine/orm": "^2.5",
    "easycorp/easy-log-handler": "^1.0",
    "nelmio/cors-bundle": "^1.5.6",
    "phpoffice/phpword": "^0.18",
    "sensio/framework-extra-bundle": "^5.3.1",
    "stof/doctrine-extensions-bundle": "^1.3.0",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^4.0.15",
    "symfony/cache": "^4.0.15",
    "symfony/console": "^4.0.15",
    "symfony/dotenv": "^4.0.15",
    "symfony/flex": "v1.21.6",
    "symfony/form": "^4.0.15",
    "symfony/framework-bundle": "^4.0.15",
    "symfony/http-kernel": "^4.0.15",
    "symfony/lts": "^4@dev",
    "symfony/maker-bundle": "1.0",
    "symfony/monolog-bundle": "^3.8",
    "symfony/polyfill-apcu": "1.6.0",
    "symfony/routing": "^4.0.15",
    "symfony/security-bundle": "^4.0.15",
    "symfony/security-csrf": "^4.0.15",
    "symfony/swiftmailer-bundle": "3.2.1",
    "symfony/translation": "^4.0.15",
    "symfony/twig-bundle": "^4.0.15",
    "symfony/validator": "^4.0.15",
    "symfony/var-dumper": "^4.0.15",
    "symfony/web-server-bundle": "^4.0.15",
    "symfony/yaml": "^4.0.15",
    "zendframework/zend-escaper": "^2.6",
    "zendframework/zend-stdlib": "^3.2",
    "zendframework/zend-validator": "^2.12.2"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "2.3.0",
    "symfony/phpunit-bridge": "^4.0.15",
    "symfony/stopwatch": "^4.0.15",
    "symfony/web-profiler-bundle": "^4.0.15",
    "symfony/web-server-bundle": "^4.0.15"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true
    }
  }
}
